Bike1 = read.csv("day.csv")
Bike1$season <- as.factor(Bike1$season)
Bike1$dteday <- as.POSIXct(Bike1$dteday)
Bike1$yr <- as.factor(Bike1$yr)
Bike1$mnth <- as.factor(Bike1$mnth)
Bike1$holiday <- as.factor(Bike1$holiday)
Bike1$weekday <- as.factor(Bike1$weekday)
Bike1$workingday <- as.factor(Bike1$workingday)
Bike1$weathersit <- as.factor(Bike1$weathersit)
summary(Bike1)
str(Bike1)
print("NA elements")
sapply(Bike1, function (x) sum(is.na(x)))
print("Null elements")
sapply(Bike1, function (x) sum(is.null(x)))
pairs(Bike1[, c(2,16)])
print("In general there is more rentals in the 2nd year. Rentals seems to follow a trend described below.")
pairs(Bike1[, c(3,5,16)])
print("Here is more clearly that for both years more bikes are hired during warmer seasons/months.")
pairs(Bike1[, c(9,10,11,16)])
print("No wonder that temperature and weather situation has positive effect on the number of bikes rented.")
pairs(Bike1[, c(12,13,16)])
print("However, humidity and windspeed have no obvious effect on the count.")
model <- lm(cnt ~ temp, data = Bike1)
summary(model)
